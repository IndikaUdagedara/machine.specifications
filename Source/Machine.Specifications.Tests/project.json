{
    "version": "1-*",
    "buildOptions": {
        "nowarn": [
            "169"
        ],
        "define": [
            "DEBUG",
            "TRACE"
        ]
    },
    "testRunner": "nunit",
    "frameworks": {
        "net461": {
            "dependencies": {
                "Machine.Specifications.Runner.Console": {
                    "version": "0.*",
                    "type": "build"
                },
                "Microsoft.NETCore.Platforms": {
                    "version": "*",
                    "type":"build"
                }
            }
        },
        "netcoreapp1.0": {
            "imports": ["dotnet"], // for dotnet-test-nunit
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.*",
                    "type": "platform"
                },
                "System.Reflection": "4.*",
                "System.Reflection.Extensions": "4.*",
                "System.Reflection.TypeExtensions": "4.*"
            }
        }
    },
    "dependencies": {
        "Machine.Specifications": {
            "target": "project"
        },
        "Example": {
            "target": "project"
        },
        "Example.Random": {
            "target": "project"
        },
        "FluentAssertions": "4.*",
        "NUnit": "3.*",
        "dotnet-test-nunit":"3.4.0-*"
    }
}